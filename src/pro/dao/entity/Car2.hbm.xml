<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
    <class name="pro.dao.entity.Car2" table="carinfo2">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment"></generator>
        </id>
        <property name="stream" type="java.lang.String" column="adress"/>
        <property name="photo" type="java.lang.String" column="picturepath"/>
                <property name="lane" type="java.lang.String">
            <column name="roadnumber" />
                    </property>
                 <property name="carnumber" type="java.lang.String" column="carnumber"/>
        <property name="datetime" type="java.sql.Timestamp">
            <column name="time" />
        </property>
		<property name="ischaozhong" type="java.lang.String" column="ischaozhong"/>
	  <property name="axis" type="java.lang.String" column="zhounumber"/>
	 <property name="weight" type="java.lang.Double" column="weight"/>
        <property name="velocity" type="java.lang.String">
            <column name="speed" />
        </property>
     <property name="ispicture" type="java.lang.String" column="ispicture"/>
	          <property name="zhou1" type="java.lang.String" column="zhou1"/>
		<property name="zhou2" type="java.lang.String" column="zhou2"/>
		<property name="zhou3" type="java.lang.String" column="zhou3"/>
		<property name="zhou4" type="java.lang.String" column="zhou4"/>
		<property name="zhou5" type="java.lang.String" column="zhou5"/>
		<property name="zhou6" type="java.lang.String" column="zhou6"/>
		<property name="zhou7" type="java.lang.String" column="zhou7"/>
		<property name="zhou8" type="java.lang.String" column="zhou8"/>
		<property name="flag" type="java.lang.String" column="flag"/>
		<property name="videopath" type="java.lang.String" column="videopath"/>
		
  

		
        
        
		
    </class>
</hibernate-mapping>
